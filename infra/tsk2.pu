@startuml

' hide the spot
hide circle

' avoid problems with angled crows feet
skinparam linetype ortho

entity "tasks" as e01 {
  tasks_id : long
  --
  title : string 
  created_at : datetime
  update_at : datetime 
}

entity "challenge_history" as e02 {
  challenge_history_id : long
  --
  task_id : long <<FK>>
  challenge_date : date
  created_at : datetime
  update_at : datetime 
}

e01 ||..o{ e02

@enduml